# Test environment configuration
# Ensures no real API calls are made during testing

# Alpaca API (use test/mock values)
ALPACA_API_KEY_ID=test_key_id
ALPACA_API_SECRET_KEY=test_secret_key
ALPACA_BASE_URL=https://mock-api.test.com
ALPACA_DATA_URL=https://mock-data.test.com

# Development flags
NEXT_PUBLIC_DEVELOPMENT_MODE=true
NEXT_PUBLIC_SUPPRESS_API_ERRORS=true
NEXT_PUBLIC_MOCK_DATA=true

# Node environment
NODE_ENV=test

# Disable real network requests
DISABLE_NETWORK_REQUESTS=true
EOF < /dev/null